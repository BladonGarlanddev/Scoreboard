{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/types/**/*.ts",
    "react-native.config.js",
    "global.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
